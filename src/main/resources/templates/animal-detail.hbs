
{{#partial "content"}}
    <br>
    <br>
    <table class="table table-dark">
        <thead class="head-dark">
        <tr>
            <th scope="col">Animal Name</th>
            <th scope="col">Animal Age</th>
            <th scope="col">Animal Health</th>
            <th scope="col">Animal Species</th>
        </tr>
        </thead>
        <tbody>

        {{#each animals}}
            <tr>
                <td>{{name}}</td>
                <td>{{age}}</td>
                <td>{{health}}</td>
                <td>{{type}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
{{/partial}}
{{> layout.hbs}}